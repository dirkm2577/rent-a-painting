<!DOCTYPE html>
<html>
  <head>
    <title>Rent a Painting</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= javascript_include_tag "/builds/application,js", "data-turbo-track": "reload", defer: true %>
  </head>

  <body>
    <%= render 'shared/navbar' %>
    <%= render 'shared/card' %>
    <%= render 'shared/grid' %>
    <%= render 'shared/footer' %>
    <%= render "shared/flashes" %>
    <%= yield %>
  </body>
</html>
